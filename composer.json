{
  "name": "monkeyscloud/monkeyslegion-core",
  "description": "Core runtime (kernel, events, helpers) for the MonkeysLegion PHP framework.",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.4",
    "psr/container": "^2.0",
    "psr/log": "^3.0",
    "psr/http-message": "^2.0",
    "psr/http-server-handler": "^1.0",
    "psr/http-server-middleware": "^1.0",
    "psr/http-factory": "^1.1",
    "monkeyscloud/monkeyslegion-http": "^1.0",
    "monkeyscloud/monkeyslegion-router": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "MonkeysLegion\\Core\\": "src/"
    },
    "files": [
      "src/Support/helpers.php"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0.x-dev"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1"
  }
}
